{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent-props": [2, 4],
    "react/react-in-jsx-scope": [0],
    "object-curly-spacing": ["error", "always"],
    "no-unused-vars": [1],
    "max-len": [0],
    "@typescript-eslint/no-non-null-assertion": [0]
  }
}
